<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: lime;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.CorrectAnswerController">
   <children>
      <Label text="Correct Answer">
         <font>
            <Font size="40.0" />
         </font></Label>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Great Job!!!">
         <font>
            <Font size="20.0" />
         </font></Text>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button mnemonicParsing="false" text="Back" />
            <Button mnemonicParsing="false" text="Next" />
         </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import java.net.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.MultipleChoiceController">
   <children>
      <Label fx:id="lbl_title" layoutX="24.0" layoutY="24.0" text="Welcome"/>
      <Label fx:id="lbl_type" layoutX="24.0" layoutY="24.0" text="Multiple Choice"/>
      <Label fx:id="lbl_question" layoutX="24.0" layoutY="24.0" text=""/>
      <Button fx:id="btn_answerA" text="" onAction="#clickedButtonA"/>
      <Button fx:id="btn_answerB" text="" onAction="#clickedButtonB"/>
      <Button fx:id="btn_answerC" text="" onAction="#clickedButtonC"/>
      <Button fx:id="btn_answerD" text="" onAction="#clickedButtonD"/>
      <Label fx:id="lbl_feedback" layoutX="24.0" layoutY="24.0" text=""/>
      <Button fx:id="returnHomeButton" text="Exit" onAction="#switchToUserHomePage"/>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
</VBox>
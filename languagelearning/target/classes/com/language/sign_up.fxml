<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import java.net.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.SignUpController">
   <children>
      <Label text="Sign Up Page" />
      <Label text="First Name" />
      <TextArea fx:id="txt_firstname" prefHeight="20.0" prefWidth="200.0" scaleY="1" />
      <Label text="Last Name" />
      <TextArea fx:id="txt_lastname" prefHeight="20.0" prefWidth="200.0" scaleY="1" />
      <Label text="UserName" />
      <TextArea fx:id="txt_username" prefHeight="20.0" prefWidth="200.0" scaleY="1" />
      <Label text="Password" />
      <TextArea fx:id="txt_password" prefHeight="20.0" prefWidth="200.0" scaleY="1" />
      <Button fx:id="signUpButton" onAction="#switchToUserHomePage" text="Sign Up" />
      <Label fx:id="lbl_error" layoutX="212.0" layoutY="235.0" prefHeight="16.0" prefWidth="170.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
</VBox>
